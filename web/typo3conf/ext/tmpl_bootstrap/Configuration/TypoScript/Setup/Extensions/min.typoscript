plugin.tx_min.tinysource {
    enable = 1
    head {
        stripTabs = 0
        stripNewLines = 0
        stripDoubleSpaces = 1
        stripTwoLinesToOne = 1
    }
    body {
        stripComments = 1
        stripTabs = 1
        stripNewLines = 1
        stripDoubleSpaces = 1
        stripTwoLinesToOne = 0
        preventStripOfSearchComment = 1
    }
    oneLineMode = 1
}